extends: [
  'eslint:recommended'
]
rules:
  no-unused-vars: [
      "error",
      { "vars": "all", "args": "none" }
  ]
  no-console: 0
  indent: [
      "error",
      2
  ]
  linebreak-style: [
      "error",
      "unix"
  ]
  quotes: [
      "error",
      "single"
  ]
  semi: [
      "error",
      "always"
  ]
globals:
  require: true
  console: true
  global: true
  exports: true
  module: true
  process: true
  describe: true
  before: true
  after: true
  it: true
  setTimeout: true
